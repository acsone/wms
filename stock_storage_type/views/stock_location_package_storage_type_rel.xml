<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="location_package_storage_type_rel_tree_view" model="ir.ui.view">
        <field name="name">stock.location.package.type.rel.tree.view</field>
        <field name="model">stock.location.package.type.rel</field>
        <field name="arch" type="xml">
            <tree editable="top">
                <field name="package_type_id" />
                <field name="location_storage_type_id" />
            </tree>
        </field>
    </record>
    <record id="location_package_type_rel_action" model="ir.actions.act_window">
        <field name="name">Storage Types Mapping</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">stock.location.package.type.rel</field>
        <field name="view_mode">list</field>
    </record>
</odoo>
